{
  "name": "benwinding-blog",
  "version": "0.0.6",
  "scripts": {
    "clean": "rm -rf public* && hexo clean",
    "start": "hexo server",
    "copy-feed": "cp ./public/content.json ./public/feed.json",
    "deploy": "dokku-pages deploy -g dokku@benwinding.com:blog -d public/",
    "build": "hexo generate",
    "drafts": "hexo server --drafts"
  },
  "hexo": {
    "version": "4.2.1"
  },
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "hexo": "^4.2.1",
    "hexo-admin": "^2.3.0",
    "hexo-browsersync": "^0.3.0",
    "hexo-cli": "^3.1.0",
    "hexo-deployer-git": "^2.1.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^2.2.0",
    "hexo-generator-index": "^1.0.0",
    "hexo-generator-json-content": "^4.2.3",
    "hexo-generator-tag": "^1.0.0",
    "hexo-reference": "^1.0.4",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^2.0.0",
    "hexo-renderer-stylus": "^1.1.0",
    "hexo-server": "^1.0.0",
    "hexo-tag-twitter": "^1.0.1"
  },
  "devDependencies": {
    "dokku-pages": "^1.0.1"
  }
}
